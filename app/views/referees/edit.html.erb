<% setTitle("Edit Referee") %>
<div class="row">
    <div class="container center display-container col-md-offset-3 col-md-6">
        <h1 class="headings new-heading row left">Referee Information</h1>
        <%= form_for @referee do |f| %>
            <%= render 'shared/error_messages', object: @referee %>
            <%= render 'fields', f: f %>

            <%= f.submit 'Update Referee', class: 'btn btn-danger' %>
        <% end %>
    </div>
</div>
