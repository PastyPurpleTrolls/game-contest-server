<div class="row">
    <div class="container center display-container col-md-offset-3 col-md-6">
        <h1 class="headings new-heading col-xs-10 left">Player Information</h1>
        <div class="form-group row left">
            <div class="col-xs-4">Contest:</div>
            <div class="col-xs-8 info-link"><%= link_to @player.contest.name, @player.contest %></div>
        </div>
        <%= form_for @player do |f| %>
            <%= render 'shared/error_messages', object: @player %>
            <%= render 'fields', f: f %>
            <%= f.submit 'Update Player', class: 'btn btn-warning' %>
        <% end %>
    </div>
</div>