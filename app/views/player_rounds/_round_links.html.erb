<% if !player_round.nil? %>
	<% @tmp = [] %>
	<% player_round.round.players.each do |p|%>
		<% @tmp << p.name %>
	<% end %>

	<li>
		<%= link_to @tmp.to_sentence, round_path(player_round.round) %>
		<% if player_round.result.nil? %>
			<span class="xs-font">(Waiting)</span>
		<% else %>
			<span class="xs-font">(<%= player_round.result %>)</span>
		<% end %>
	</li>

<% end %>
