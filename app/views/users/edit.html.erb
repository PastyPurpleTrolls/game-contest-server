<% setTitle("Edit Account") %>
<div class="row">
    <div class="container center display-container col-md-offset-3 col-md-6">
        <h1 class="headings new-heading row left">Account Information</h1>
        <%= form_for(@user) do |f| %>
            <%= render 'fields', f: f %>
            <%= f.submit 'Update Account', class: 'btn btn-warning' %>
        <% end %>
    </div>
</div>
