<div class="row">
    <div class="container center display-container col-md-offset-3 col-md-6">
        <h1 class="headings new-heading left row">Contest Information</h1>
        <%= form_for @contest do |f| %>
            <%= render 'shared/error_messages', object: @contest %>
            <%= render 'fields', f: f %>
            <%= f.submit 'Create Contest', class: 'btn btn-warning' %>
        <% end %>
    </div>
</div>
