<div class="info-container">
    <% if params[:page].nil? %>
        <%= link_to "Round #{round_counter + 1}", round_path(round) %>&nbsp;&nbsp;
        <% if round.winners.length > 0 %>
            <%= image_tag 'crown.png', class: 'xs-crown' %>
            <span class="text-xs"><%= round.winners %></span>
            <%= image_tag 'crown.png', class: 'xs-crown' %>
        <% else %>
            <span class="text-xs">Tie</span>
        <% end %>
    <% else %>
        <%= link_to "Round #{round_counter + 1 + (params[:page].to_i - 1) * 10}", round_path(round) %>&nbsp;&nbsp;
        <% if round.winners.length > 0 %>
            <%= image_tag 'crown.png', class: 'xs-crown' %>
            <span class="text-xs"><%= round.winners %></span>
            <%= image_tag 'crown.png', class: 'xs-crown' %>
        <% else %>
            <span class="text-xs">Tie</span>
        <% end %>
    <% end %>
</div> 
